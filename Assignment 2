class Vehicle:
    def move(self):
        pass  # Base method (could be abstract)

class Car(Vehicle):
    def move(self):
        print("Driving ğŸš—")

class Plane(Vehicle):
    def move(self):
        print("Flying âœˆï¸")

class Boat(Vehicle):
    def move(self):
        print("Sailing â›µ")

# Polymorphism in action
def make_it_move(vehicle):
    vehicle.move()

vehicles = [Car(), Plane(), Boat()]

for v in vehicles:
    make_it_move(v)
